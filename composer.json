{
    "name": "faustbrian/affilinet-php-client",
    "description": "A developer-experience focused Affilinet API client.",
    "keywords": ["affilinet", "sdk", "php", "Affilinet-PHP-Client", "Affilinet PHP Client", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.1",
        "nesbot/carbon": "^1.21",
        "illuminate/cache": "5.4.* || 5.5.*",
        "illuminate/container": "5.4.* || 5.5.*",
        "illuminate/filesystem": "5.4.* || 5.5.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\AffilinetSdk\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\AffilinetSdk\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
